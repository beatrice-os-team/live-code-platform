def add(a, b):
    return a + b

def multiply(a, b):
    return a * b

def calculate():
    let x = 10
    let y = 20
    let sum = add(x, y)
    let product = multiply(x, y)
    
    print("x = " & str(x))
    print("y = " & str(y))
    print("x + y = " & str(sum))
    print("x * y = " & str(product))
    
    if sum > product:
        print("和大于积")
    else:
        print("积大于等于和")

calculate()

